{
    "clangd.path": "/usr/bin/clangd",
    "go.goplsPath": "~/go/bin/gopls",
    "languageserver": {
        "clangd": {
            "command": "/usr/bin/clangd",
            "args": ["--background-index"],
            "rootPatterns": ["compile_commands.json", ".git/", ".hg/"],
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        }
    }
}
